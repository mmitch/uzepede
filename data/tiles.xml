<?xml version="1.0" ?>
<gfx-xform version="1">
  <input file="tiles.raw" type="raw" tile-width="6" tile-height="8" width="60" height="48"/>
  <output file="tiles.inc">
    <tiles var-name="Tiles"/>
    <maps pointers-size="16">
      <map var-name="t_black"         left="0" top="0" width="1" height="1"/>
      <map var-name="t_wormbody"      left="1" top="0" width="1" height="1"/>
      <map var-name="t_wormheadright" left="2" top="0" width="1" height="1"/>
      <map var-name="t_wormheadleft"  left="3" top="0" width="1" height="1"/>
      <map var-name="t_spider"        left="4" top="0" width="1" height="1"/>
      <map var-name="t_bee"           left="5" top="0" width="1" height="1"/>
      <map var-name="t_bug"           left="6" top="0" width="1" height="1"/>
      <map var-name="t_player"        left="0" top="1" width="1" height="1"/>
      <map var-name="t_shot"          left="1" top="1" width="1" height="1"/>
      <map var-name="t_mushroom1"     left="2" top="1" width="1" height="1"/>
      <map var-name="t_mushroom2"     left="3" top="1" width="1" height="1"/>
      <map var-name="t_mushroom3"     left="4" top="1" width="1" height="1"/>
      <map var-name="t_gameover"      left="0" top="3" width="10" height="1"/>
      <map var-name="t_title"         left="0" top="4" width="8" height="1"/>
      <map var-name="t_pressstart"    left="0" top="5" width="10" height="1"/>
    </maps>
  </output>
</gfx-xform>
